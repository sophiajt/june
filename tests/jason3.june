// output: 33

struct Stats {
    age: i64
}

struct allocator Employee {
    name: string,
    stats: Stats,
}

fun helper(mut employee: Employee) {
    for x in 1..100000 {
        employee.stats = new shared Stats(age: 100)
    }
}

fun main() {
    helper(new shared Employee(name: "foo", new shared Stats(age: 100)))
}