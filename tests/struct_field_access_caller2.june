// output: 22

struct Stats {
    age: i64
}

struct Employee {
    name: string,
    stats: shared Stats,
}

fun helper() -> shared Stats {
    let stats = new shared Stats(age: 22)
    let employee = new shared Employee(name: "Sally", stats: stats)

    return employee.stats
}

fun main() {
    println(helper().age)
}