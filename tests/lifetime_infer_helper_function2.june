// output: 33

struct Stats {
    age: i64
}

struct allocator Employee {
    name: string,
    stats: Stats,
}

fun create_stats() -> Stats {
    return new shared Stats(age: 33)
}

fun helper(mut employee: shared Employee) {
    employee.stats = create_stats()
}

fun main() {
    mut employee = new shared Employee(name: "Sophia", stats: new shared Stats(age: 22))

    helper(employee)

    println(employee.stats.age)
}