struct Stats {
    age: i64
}

struct Employee {
    stats: Stats
}

fun get_employee() -> Employee {
    mut employee = new shared Employee(stats: new shared Stats(age: 100));
    let employee2 = new shared Employee(stats: new shared Stats(age: 200));

    if true {
        return employee
    }
    employee.stats = employee2.stats;
}

fun main() {
    let employee = get_employee()
}