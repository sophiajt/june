// output: 99

struct Stat {
    width: i64
}

struct allocator Box {
    stat: shared Stat

    fun recreate_stat(mut self) {
        .stat = new shared Stat(width: 99)
    }
}

fun main() {
    let box = new shared Box(stat: new shared Stat(width: 22))
    box.recreate_stat()

    println(box.stat.width)
}