// output: 33

struct Stats {
    age: i64
}

struct Employee {
    name: string,
    stats: shared Stats,
}

fun helper() -> shared Stats {
    return new shared Stats(age: 33)
}

fun main() {
    mut employee = new shared Employee(name: "Sophia", stats: new shared Stats(age: 22))

    employee.stats = helper()

    println(employee.stats.age)
}